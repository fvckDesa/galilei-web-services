            "required": ["kind"],
            "properties": {
              "kind": {
                "type": "string",
                "enum": ["InternalError"]
              }
            }
          }
        ]
      },
      "AppServiceSchema": {
        "type": "object",
        "required": ["name", "replicas", "image", "port"],
        "properties": {
          "image": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "port": {
            "type": "integer",
            "format": "int32"
          },
          "replicas": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "AuthData": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Password"
          },
          {
            "type": "object",
            "required": ["username", "remember"],
            "properties": {
              "remember": {
                "type": "boolean"
              },
              "username": {
                "type": "string"
              }
            }
          }
        ]
      },
      "ErrorMessage": {
