        "type": "object",
        "properties": {
          "image": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "port": {
            "type": "integer",
            "format": "int32"
          },
          "replicas": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "PartialProjectSchema": {
        "type": "object",
        "properties": {
